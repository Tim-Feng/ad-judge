<div id="video_column" class="span5 well">
  <h4 class="one-line-title"><%= link_to post.title, post_path(post) %></h4>
    <iframe width="360" height="202" src="//www.youtube.com/embed/<%== fix_embedded_url(post.url) %>" frameborder="0" allowfullscreen></iframe>
  <hr>
  <%= render 'shared/progress_bar', post: post %>
  <%= render 'shared/vote_buttons', post: post %>
</div>

