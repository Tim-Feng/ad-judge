<% content_for :specific_og_metas do %>
  <meta property="og:image" content="https://s3.amazonaws.com/ad-judge-assets/facebook_logo_square.jpg" >
    <meta property="og:url" content="http://www.adjudge.com" /> 
  <meta property="og:title" content="Ads Worth Spreading" />
  <meta property="og:description" content="Ad Judge" />
<% end %>

<div id="main_items" class="center main_items_decoration">
  <% @posts.each do |post| %>
    <%= render 'post', post: post %>
  <% end %>

<div class="paginate">
  <%= paginate @posts, remote: true %>
</div>