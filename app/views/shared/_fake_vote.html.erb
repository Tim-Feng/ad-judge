<div class='center vote_buttons_placement'>
  <%= link_to fake_vote_post_path(post), method: 'post', remote: true, class: 'btn btn-lg btn-success' do %>
    <span class='glyphicon glyphicon-thumbs-up black'></span>
  <% end %>
  <%= link_to fake_vote_post_path(post), method: 'post', remote: true, class: 'btn btn-lg btn-danger' do %>
    <span class='glyphicon glyphicon-thumbs-down black'></span>
  <% end %>
</div>